{% extends 'base.html.twig' %}

{% block title %}{{ caption }}{% endblock %}

{% block body %}
    {{ parent() }}
<main>
    <section class="normal-form">
        <h1>{{ caption }}</h1>
        {{ form(form) }}
        {% if message is defined %}
            <span class="ok-message">{{ message }}</span>
        {% endif %}
    </section>
</main>
{% endblock %}
