{% extends 'base.html.twig' %}
{% block title %}{{ caption }}{% endblock %}

{% block body %}
    {{ parent () }}
    <main>
        <section class="normal-form">
            <h1>{{ caption }}</h1>
            {{ form(form) }}
            {% if error is defined %}
                <div id="error">
                    {{ error }}
                </div>
            {% endif %}
        </section>
    </main>
{% endblock %}
