{% extends 'base.html.twig' %}
{% set caption = 'Dodawanie katalogu' %}
{% block title %}{{ caption }}{% endblock %}

{% block body %}
    {{ parent() }}
    <main>
        <section class="normal-form">
            <h1>{{ caption }}</h1>
            {{ form(form) }}
            {% if error is defined %}
                <span id="error">{{ error }}</span>
            {% endif %}
        </section>
    </main>
{% endblock %}
